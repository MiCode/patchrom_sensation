    <string name="scene_description_panorama">Brug til at optage <b>panoramabilleder</b>, så du kan optage en hel scene med et billede.</string>
    <string name="scene_description_hdr">Brug til at optage <b>motiver under lyse betingelser</b>, så dine billeder ikke vises med høj lyskontrast.</string>
    <string name="scene_description_burst">Brug til at optage <b>flere billeder i en hurtig serie</b>, så du ikke går glip af det perfekte billede.</string>
    <string name="scene_description_night">Brug til at optage <b>billeder om natten</b>, så dine billeder står tydelige i lav belysning.</string>
    <string name="scene_description_action">Brug til at optage <b>motiver, der bevæger sig hurtigt</b>, så billedet forbliver skarpt.</string>
    <string name="scene_description_macro">Brug til at optage <b>motiver tæt på</b>, så du kan optage selv små detaljer i motivet.</string>
    <string name="scene_description_portrait">Brug til at optage <b>en enkeltperson</b>, at motivet står skarpt og i fokus, og baggrunden er sløret.</string>
    <string name="scene_description_lowlight">Brug ved <b>lavt belysningsniveau</b> til håndholdte billeder uden blitz, der ser mere naturligt ud.</string>
